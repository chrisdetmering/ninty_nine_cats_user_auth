<h1>Sign-up!</h1>


<form action="<%= users_url %>" method="post">
 <input
  name="authenticity_token"
  type="hidden"
  value="<%= form_authenticity_token %>">


<label for="user_name">User Name:</label> 

<input 
  type="text" 
  id="user_name" 
  name="user[user_name]"
  value="<%=@user.user_name%>"><br>

<label for="password">Password:</label>

<input 
  type="text" 
  id="password" 
  name="user[password]"
  value="<%=@user.password%>"><br>

<input type="submit">
</form>